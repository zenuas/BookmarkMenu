{
	"name": "BookmarkMenu",
	"description": "bookmark-menu from toolbar",
	"version": "2025.07.21",
	"manifest_version": 3,
	"permissions": ["bookmarks", "favicon", "tabs"],
	"action":
	{
		"default_popup": "main.html",
		"default_icon": "star_32dp_E3E3E3_FILL0_wght400_GRAD0_opsz40.png"
	},
	"web_accessible_resources":
	[
		{
			"resources": ["_favicon/*"],
			"matches": ["<all_urls>"],
			"extension_ids": ["*"]
		}
	]
}
